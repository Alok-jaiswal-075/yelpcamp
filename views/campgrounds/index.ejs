<% layout('./layouts/boilerplate') %> 
<h1>All camp-grounds will be shown here</h1>
<% for(let camp of camps) {%>
    <div class="card mb-3" >
        <div class="row g-0">
          <div class="col-md-4">
            <img src="<%=camp.images[0].url %> " class="img-fluid rounded-start" alt="...">
          </div>
          <div class="col-md-8">
            <div class="card-body">
              <h5 class="card-title"><%=camp.title%></h5>
              <p class="card-text"><%=camp.discription %> </p>
              <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
              <a href="/campgrounds/<%=camp._id %> " class="btn btn-primary"><%=camp.title %> </a>
            </div>
          </div>
        </div>
      </div>
<% } %>  
